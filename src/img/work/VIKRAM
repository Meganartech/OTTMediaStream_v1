        function createAdBox(containerElement, adId, divId) {
            if (containerElement) {
                var divAdBox = document.createElement("div");
                divAdBox.className = "adbox side-top";
                
                var divGpt = document.createElement("div");
                divGpt.id = "div-gpt-ad-" + adId;
                
                divAdBox.appendChild(divGpt);
                containerElement.appendChild(divAdBox);
            }
        }

        function createAdElements() {
            createAdBox(RightAd1Element, "Right1", "div-gpt-ad-Right1");
            createAdBox(RightAd2Element, "Right2", "div-gpt-ad-Right2");

            if (RightAd3Element) {
                createAdBox(RightAd3Element, "Right3", "div-gpt-ad-Right3");
